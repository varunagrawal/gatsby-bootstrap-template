    "gatsby-plugin-remove-trailing-slashes": "^2.3.11",
    "gatsby-plugin-sass": "^2.3.12",
    "jquery": "^3.5.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-refresh": "^0.8.3"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/varunagrawal/gatsby-bootstrap-template"
  },
  "bugs": {
    "url": "https://github.com/varunagrawal/gatsby-bootstrap-template/issues"
